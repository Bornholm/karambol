{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "font-awesome/font-awesome",
        "version": "4.6.3",
        "dist": {
          "url": "https://fortawesome.github.io/Font-Awesome/assets/font-awesome-4.6.3.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "silex/silex": "^1.3",
    "twig/twig": "^1.24",
    "symfony/twig-bridge": "^3.0",
    "symfony/yaml": "^3.0",
    "monolog/monolog": "^1.17",
    "symfony/security": "^3.0",
    "components/jquery": "^2.2",
    "twbs/bootstrap": "^3.3",
    "doctrine/orm": "^2.5",
    "symfony/translation": "^3.0",
    "symfony/config": "^3.0",
    "symfony/form": "^3.0",
    "symfony/validator": "^3.0",
    "symfony/security-csrf": "^3.0",
    "symfony/event-dispatcher": "^3.0",
    "symfony/property-access": "^3.0",
    "symfony/expression-language": "^3.0",
    "font-awesome/font-awesome": "4.6.3",
    "cocur/slugify": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Karambol\\": "src/",
      "": "plugins/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Karambol\\Script\\AssetsLinker::linkAssets"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "symfony/var-dumper": "^3.0"
  },
  "config": {
    "secure-http": false
  }
}
